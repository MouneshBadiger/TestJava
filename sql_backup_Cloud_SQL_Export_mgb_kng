DROP TABLE IF EXISTS `package_rate`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `package_rate` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `created_by` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `is_active` bit(1) DEFAULT NULL,
  `modified_by` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `modified_date` datetime DEFAULT NULL,
  `rate` double DEFAULT NULL,
  `channel_pack` int(11) DEFAULT NULL,
  `tenant_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FK3xoe0ta740mvgfaor976nl2vn` (`channel_pack`),
  KEY `FKltm38ns6gdysgdrpgbdutdljt` (`tenant_id`),
  CONSTRAINT `FK3xoe0ta740mvgfaor976nl2vn` FOREIGN KEY (`channel_pack`) REFERENCES `channel_pack` (`id`),
  CONSTRAINT `FKltm38ns6gdysgdrpgbdutdljt` FOREIGN KEY (`tenant_id`) REFERENCES `tenant` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_bin;
/*!40101 SET character_set_client = @saved_cs_client */;

