--
-- Table structure for table `post`
--

DROP TABLE IF EXISTS `post`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `post` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `content` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `created_by` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `is_active` bit(1) DEFAULT NULL,
  `last_modified_by` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `last_modified_date` datetime DEFAULT NULL,
  `post_type` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `user_id` int(11) DEFAULT NULL,
  `tital` varchar(255) COLLATE utf8mb4_bin DEFAULT NULL,
  `content1` text COLLATE utf8mb4_bin,
  `gps_json` varchar(500) COLLATE utf8mb4_bin DEFAULT NULL,
  `lattitude` double DEFAULT NULL,
